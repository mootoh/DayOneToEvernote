				<dict>
					<key>COMMAND_STRING</key>
					<string>require 'rubygems'
require 'sqlite3'

to_export = ARGF.first.to_i

require 'etc'
HOME = Etc.getpwuid.dir

DAYONE_JOURNAL_PATH = HOME + "/Library/Containers/com.dayoneapp.dayone/Data/Library/Application Support/Journal.sqlite"
db = SQLite3::Database.new(DAYONE_JOURNAL_PATH)

row = db.execute('select Z_PK, Z_ENT, Z_OPT, ZCREATIONYEAR, ZISENTRYDELETED,ZSTARRED,ZCREATIONDATE, ZMODIFIED,ZPHOTOMODIFIED,ZENTRYTEXT, ZUUID, ZLOCATION, ZWEATHER from ZJOURNALENTRY where ZSTARRED=1 order by ZCREATIONDATE DESC')[to_export]

creation_date = DateTime.strptime(row[6].to_s, '%s')
creation_date = Time.parse(creation_date.to_s).strftime("%m/%d/%Y %I:%M:%S %p")

puts creation_date, row[3], row[9]</string>
