				<dict>
					<key>source</key>
					<string>on run {input, parameters}	set theDate to date (item 1 of input)	set theYear to item 2 of input	set theTitle to item 3 of input	set body to ""	repeat with cur from 4 to count input		set body to body &amp; (item cur of input) &amp; ""	end repeat		set year of theDate to theYear		tell application "Evernote"		set theNote to create note title theTitle with text body as text		set creation date of theNote to theDate	end tell		return inputend run</string>
